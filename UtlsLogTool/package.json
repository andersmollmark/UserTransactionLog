{
  "name": "UtlLogTool",
  "version": "1.0.0",
  "description": "",
  "main": "main.ts",
  "scripts": {
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "postinstall": "typings install",
    "start": "electron main.ts",
	"clean": "rm -rf ./dist",
    "clean-win": "rm -rf ./dist/windows",
    "clean-lin": "rm -rf ./dist/linux",
    "clean-osx": "rm -rf ./dist/osx",
    "package-linux": "npm run clean-lin && electron-packager . UtlsLogTool --out=dist/linux --platform=linux --arch=x64",
    "package-windows": "npm run clean-win && electron-packager . UtlsLogTool --out=dist/windows --platform=win32 --arch=all",
    "package-osx": "npm run clean-osx && electron-packager . UtlsLogTool --out=dist/osx --platform=darwin --arch=x64"
  },
  "author": "AndersMollmark",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^2.2.0",
    "css-loader": "^0.26.1",
    "electron": "^1.4.1",
    "electron-packager": "^8.4.0",
    "es6-shim": "^0.34.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.7.2",
    "typescript": "^2.1.4",
    "typings": "^1.3.2",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "@types/lodash": "^4.14.45",
    "angular-in-memory-web-api": "~0.2.2",
    "angular2-datatable": "^0.5.2",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "immutable": "^3.7.6",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.7.4"
  }
}
